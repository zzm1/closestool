/* pages/setting/setting.wxss */
@import "/pages/template/mySlider.wxss";

.setting {
    display: flex;
    flex-direction: column;
}

.list-com {
    padding: 0 32rpx;
    border-top: 1px solid #e1e1e1;
    border-bottom: 1px solid #e1e1e1;
    margin-bottom: 20rpx;
    background: #fff;
    font-size: 34rpx;
}

.setList1 {
    margin-top: 20rpx;
    background: #fefefe;
    line-height: 88rpx;
}

.setList2 {
    display: flex;
    height: 120rpx;
    align-items: center;
    justify-content: space-between;
    font-size: 32rpx;
}

