.setList4 {
    margin-bottom: 20rpx;
    border-top: 1px solid #e1e1e1;
    border-bottom: 1px solid #e1e1e1;
}

.setList5 {
    padding-right: 0;
    padding: 0;
}

.setList5 image {
    width: 16rpx;
    height: 26rpx;
}

.setList5 .setList5-item {
    padding-left: 32rpx;
}

.setList5 .setList5-item view {
    line-height: 88rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1px solid #e1e1e1;
    padding-right: 32rpx;
}

.setList5 .setList5-item:last-child view {
    border-bottom: 0;
}

